{
  "name": "create-new-ui",
  "version": "0.0.1",
  "description": "A semantic, BEM-based UI framework",
  "type": "module",
  "files": [
    "dist/",
    "bin/"
  ],
  "bin": {
    "create-new-ui": "./dist/new.mjs"
  },
  "main": "./dist/new.mjs",
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node ./src/new.js",
    "build": "babel src --out-dir dist --extensions \".js\" --copy-files && mv dist/new.js dist/new.mjs",
    "prepare": "npm run build",
    "start": "node ./dist/new.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NewDesignFile/create-new-ui.git"
  },
  "keywords": [
    "foundations",
    "typography",
    "responsive",
    "type",
    "type-styles",
    "modular",
    "type",
    "tokens",
    "desktop",
    "mobile",
    "colors",
    "spacings",
    "spacing-blocks",
    "reset"
  ],
  "author": "@planetabhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NewDesignFile/create-new-ui/issues"
  },
  "homepage": "https://github.com/NewDesignFile/create-new-ui#readme",
  "dependencies": {
    "inquirer": "^12.2.0",
    "fs-extra": "^11.2.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@parcel/transformer-sass": "^2.13.2",
    "autoprefixer": "^10.4.20",
    "parcel": "^2.13.2"
  }
}
